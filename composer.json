{
	"name": "lemonink/lemonink-woocommerce",
	"description": "Woocommerce plugin for watermarking ebooks using LemonInk",
	"repositories":[
		{
			"type": "composer",
			"url": "https://wpackagist.org"
		},
		{
			"type": "composer",
			"url": "https://packagist.org"
		}
	],
	"keywords": [
		"LemonInk",
		"woocommerce-plugin",
		"wordpress-plugin",
		"watermarking",
		"api"
	],
	"homepage": "https://lemonink.co/",
	"license": "MIT",
	"authors": [
		{
			"name": "LemonInk",
			"homepage": "https://lemonink.co/"
		}
	],
	"minimum-stability": "dev",
	"require": {
		"php": ">=5.3.3",
		"lemonink/lemonink-php": ">=1.1.0",
		"wpackagist-plugin/woocommerce": "*"
	},
	"autoload": {
		"psr-0": { "LemonInk\\" : "src/" }
	},
	"config": {
		"allow-plugins": {
			"composer/installers": true
		}
	}
}
